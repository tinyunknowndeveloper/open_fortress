"VertexLitGeneric"
{
	"$baseTexture" 	"models/weapons/c_models/c_classic_medkit/c_classic_medkit_custom"
	"$bumpmap" 	"models/weapons/c_models/c_classic_medkit/c_classic_medkit_normal"
	"$phong" "1"
	"$phongexponent" "5"

	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	
	"$blendtintbybasealpha"		"1"
	//"$blendtintcoloroverbase"		"1"
	"$colortint_base"		"{0 0 0}"
	"$color2"		"{93 72 117}"
	"$colortint_tmp"		"[0 0 0]"	
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"ItemTintColor"
		{
			"resultVar"		"$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"		"$colortint_tmp"
			"srcVar2"		"$colortint_base"
			"resultVar"		"$color2"
		}
		"spy_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}


"Water"
{
	"%keywords" "tf"
	//	$forcecheap 1
	
	"%compilewater" 1
	"$abovewater" 1
	//"$nofresnel" "1"

	"$envmap" "env_cubemap"	
	"$refracttexture" "_rt_WaterRefraction"
	"$refractamount" "0.1"
	//"$refracttint" "{191 179 155}"
	"$refractblur" "1"

	"$reflecttexture" "_rt_WaterReflection"
	"$reflectamount" ".25"
	//"$reflecttint" "{230 240 255}"
//	"$reflectsaturation" "[0 0 1]"

	"$scale" "[1 1]"

//	"$bumpmap" "dev/water_dudv"
	"$normalmap" "water/tfwater001_normal"

	"$surfaceprop" "water"
	"$bottommaterial" "water/water_slime_beneath.vmt"
	"$bumpframe" "0"

	"$fogenable" 1
	"$fogcolor" "{100 240 50}"
	"$fogstart" "-100"
	"$fogend" "50"

	 "$temp" "[0 0]"
	 "$curr" 0.0
	 "$curr2" 0.0

	"Proxies"
	{
	
	"Water_DX60"
	{
		"$fallbackmaterial" "nature/water_dx70"
	}
	
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30
		}

		"Sine"
		{
			"sineperiod" "30"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "35"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}  

//		"TextureScroll"
//		{
//			"texturescrollvar" "$bumptransform"
//			"texturescrollrate" .1
//			"texturescrollangle" 45.00
//		}
		"WaterLOD"
		{
			// fixme!  This has to be here, or material loading barfs.
			"dummy" 0
		}
	}
}

// This config is executed automatically by server, you probably shouldn't touch this file

// Allows civilian to get boosted on certain maps which can utilise Civ boosts
of_teamplay_knockback 1

of_payload_override 1

of_knockback_all 1
of_knockback_bullets 1
of_knockback_explosives 1
of_knockback_melee 1

// Execute a cfg which server owner can customise
exec config_esc